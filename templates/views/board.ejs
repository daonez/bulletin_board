<div class="section">
  <table class="table table-striped">
    <thead>
      <tr>
        <th scope="col">#</th>
        <th scope="col">제목</th>
        <th scope="col">작성자</th>
        <th scope="col">작성일</th>
      </tr>
    </thead>
    <tbody>
      <% for( let elements of posts ) { %>
      <tr>
        <th scope="row"><%= elements.postNum %></th>
        <td>
          <a href="/posts/<%=elements._id%>"><p><%= elements.title %></p></a>
        </td>
        <td><%= elements.owner.author %></td>

        <td>
          <div
            class="timestamp"
            data-timestamp="<%= elements.createdAt %>"
            onload="test()"
          ></div>
        </td>
      </tr>
      <% } %>
    </tbody>
  </table>
</div>
